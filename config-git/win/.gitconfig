[alias]
	ig = "!f() { curl -L -s https://raw.githubusercontent.com/swoiow/dsc/master/.gitignore -o .gitignore ;}; f"
	ag = "!f() { eval $(ssh-agent -s) && ssh-add ~/.ssh/github && ssh-add ~/.ssh/bitbucket ;}; f"
	githubuser = "!f() { git config user.name swoiow && git config user.email swoiow@users.noreply.github.com ;}; f"
[color]
	branch  = auto
	diff  = auto
	interactive  = auto
	status = auto
